<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="database_access.IdaApplicationFormInfoTbl" table="IDA_APPL_FORM_INFO_TBL">
		<id name="applicationFromNbr" type="string" unsaved-value="null">
			<column name="APPLICATION_FORM_NBR" sql-type="char(10)" not-null="true" />
			<generator class="assigned" />
		</id>
		<property name="entryDt" type="date" column="ENTRY_DT"/>
		<property name="groupId" type="string" column="GROUP_ID" />
</class>
</hibernate-mapping>